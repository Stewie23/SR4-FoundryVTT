{{> "systems/sr4/dist/templates/v2/actor/parts/limits.hbs" }}
<div class="attributes">
    {{#each system.attributes as |attribute attributeId|}} {{#unless attribute.hidden}}
        {{> "systems/sr4/dist/templates/v2/actor/parts/attribute.hbs"
                attribute=attribute
                attributeField=(objValue @root.systemFields.attributes.fields attributeId)
                rootId=@root.rootId
        }}
    {{/unless}} {{/each}}
</div>
